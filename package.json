{
    "name": "agrims",
    "version": "1.0.0",
    "description": "Agriculture Management System",
    "main": "_app.js",
    "type": "commonjs",
    "scripts": {
        "start:dev": "nodemon --exec ./node_modules/.bin/babel-node ./src/_app.js",
        "client": "npm start --prefix client",
        "clean": "rm -rf build && mkdir build",
        "build-babel": "babel -d ./build ./src -s",
        "start": "npm run build && node ./build/_app.js",
        "build": "npm run clean && npm run build-babel",
        "client-install": "cd client && npm install",
        "lint": "eslint .",
        "data:destroy": "node ./src/data/users.js -d",
        "data:import": "node ./src/data/users.js",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
    },
    "engines": {
        "node": "17.3.0"
    },
    "keywords": [
        "Agriculture Management System",
        "Nodejs",
        "Javascript",
        "AMS"
    ],
    "author": "France Rebollos",
    "license": "MIT",
    "dependencies": {
        "-": "^0.0.1",
        "@babel/preset-react": "^7.16.7",
        "@sentry/tracing": "^6.17.7",
        "babel-cli": "^6.26.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^14.3.2",
        "express": "^4.17.2",
        "express-async-handler": "^1.2.0",
        "free-mobile-sms-api": "^1.0.4",
        "helmet": "^5.0.2",
        "history": "^5.2.0",
        "joi": "^17.6.0",
        "jsonwebtoken": "^8.5.1",
        "mysql2": "^2.3.3",
        "node-mailjet": "^3.3.5",
        "rimraf": "^3.0.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.16.8",
        "@babel/core": "^7.16.12",
        "@babel/node": "^7.16.8",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@babel/runtime": "^7.16.7",
        "@hot-loader/react-dom": "^17.0.2",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.3",
        "babel-node": "^0.0.1-security",
        "babel-plugin-module-resolver": "^4.1.0",
        "copy-webpack-plugin": "^10.2.4",
        "cross-env": "^7.0.3",
        "eslint": "^8.8.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-babel-module": "^5.3.1",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-security": "^1.4.0",
        "html-webpack-plugin": "^5.5.0",
        "method-override": "^3.0.0",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "react-hot-loader": "^4.13.0",
        "sass-loader": "^12.6.0",
        "supertest": "^6.2.2",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.4",
        "webpack-hot-middleware": "^2.25.1",
        "webpack-merge": "^5.8.0"
    }
}
